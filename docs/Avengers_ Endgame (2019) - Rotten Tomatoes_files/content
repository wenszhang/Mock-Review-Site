		var dynLib = new GyrobaseAjaxLibrary("/mods/dyn_home");
		var SpotOidToBlock;
		var CalOidToBlock ;
          
		function getCalendar(SpotOid) {
			
			
			var params = { "AdminOid": "2572", "ScriptAlias": "sacramento", "OidToBlock": CalOidToBlock, "SpotToBlock": SpotOid, "PublicKey": "1579880404", "hash": "fe3086fae7709b38444c3456ce0ee98e" };
			dynLib.runMacro("getCalendar", params, function (response) { document.getElementById('CalendarContent').innerHTML = response["output"]; CalOidToBlock = response["oid"]; });
		}  
		

		

				var passwordLib = new GyrobaseAjaxLibrary("/ajax/passwordMacros");
				var ballotLib = new GyrobaseAjaxLibrary("/ajax/ballotMacros");

			var showLogout = 0;

			function pageLoad() {
				var f3 = document.getElementById("f3");
				if (readCookie('login')) {
					
					
					var params = { "login": readCookie('login'), "PublicKey": "1579880404", "hash": "f888838e3094eafbc60e70e5477c56e9" } ;
					passwordLib.runMacro("getHandle", params, function (response) { 
						if (document.getElementById('loginForm') !== null) { 
							if (response["status"] == 1) {
								document.getElementById('tmpUser').innerHTML = '&nbsp;' + response["Handle"];
								ShowHide('loginForm','hide','');
								Effect.toggle('welcome', 'blind', {queue: 'end', duration: '.25'});
								showLogout = 1;
							} else {
								ShowHide('welcome','hide','');
								Effect.toggle('loginForm', 'blind', {queue: 'end', duration: '.25'});
							}
						}
						if (document.getElementById('CommentLoginForm') !== null) {
							if (response["status"] == 1) {
								document.getElementById('CommentUser').innerHTML = ',&nbsp;' + response["Handle"];
								document.getElementById('auth').value = readCookie('auth');
								ShowHide('CommentLoginForm','hide','');
								Effect.toggle('leaveComment', 'blind', {queue: 'end', duration: '.25'});
								showLogout = 1;
							} else {
								ShowHide('CommentLoginForm','show','');
								eraseCookie('login');
								eraseCookie('auth');
							}
						}
					}, function (error) {
						var ifrm = document.createElement("IFRAME");
						ifrm.setAttribute("src", "/sacramento/WhackBack?return=/sacramento/best-worst-of-wide/content%3Foid%3D29488708");
						ifrm.style.width = 0+"px";
						ifrm.style.height = 0+"px";
						document.body.appendChild(ifrm); 
						//alert("This page generated an error.\n\nThe page is being rebuilt, and you will be redirected to the fresh page momentarily.");
						setTimeout('window.location.reload(true);',2000);
					});
					if (showLogout == 1) {
								document.getElementById('LoginOut').innerHTML = '<a href="\/sacramento\/Login?logout=1&pub=sacramento" class="footer">logout<\/a>&nbsp;|&nbsp;<a href="/sacramento/Login?changePass=1" class="footer">change password<\/a><br \/>';
					}
				} else {
					if (document.getElementById('welcome') !== null) {
						ShowHide('welcome','hide','');
						Effect.toggle('loginForm', 'blind', {queue: 'end', duration: '.25'});
					}
					if (document.getElementById('CommentLoginForm') !== null) {
						ShowHide('CommentLoginForm','show','');
					}
				}
				if (readCookie('user') !== "" && readCookie('user') !== null) {
					if (document.getElementById("loginUserName") !== null) {
						document.getElementById("loginUserName").value = readCookie('user');
					}
				}
			}

			function logIn() {
				document.getElementById("loginForm").disabled = true;
				var userField = document.getElementById("loginUserName");
				var passField = document.getElementById("loginPassword");
				var user = userField.value;
				var pass = passField.value;
				document.getElementById("loginPassword").value = '';

					
					      
					var params = { "loginUserName": user, "loginPassword": pass, "PublicKey": "1579880404", "hash": "64ad62e56218d85ec1a73e11dc654f73" };
					passwordLib.runMacro("authenticate", params, 
						function (response) { 
							verifyLogin(response["auth"],response["user"],response["Cookie"],response["Handle"],response["status"]); 
						}, 
						function (error) {
							var ifrm = document.createElement("IFRAME");
							ifrm.setAttribute("src", "/sacramento/WhackBack?return=/sacramento/best-worst-of-wide/content%3Foid%3D29488708");
							ifrm.style.width = 0+"px";
							ifrm.style.height = 0+"px";
							document.body.appendChild(ifrm); 
							//alert("This page generated an error.\n\nThe page is being rebuilt, and you will be redirected to the fresh page momentarily.");
							setTimeout('window.location.reload(true);',2000);
						}
					);
			}

			function verifyLogin(auth,user,Cookie,Handle,status) {
				var f3 = document.getElementById("f3");
				if (auth !== "") {
					eraseCookie('login');
					createCookie('login',user + ':' + Cookie,1);
					createCookie('auth',auth,1);
					location.replace('/sacramento/local/MyNR?reload=feff0da4e19a55d36b649e1bd4d3cdf0');
				} else {
					alert("The password you entered doesn't match the password on file for the username you entered.  Please try again.");
				}
				f3.login_btn.disabled = false;
			}
				
			function logout() {
				var user = document.getElementById("loginUserName").value;
				if (document.getElementById('loginForm') !== null) {
					document.body.style.cursor = "wait";
					var f3 = document.getElementById("f3");
					
					document.getElementById("loginForm").disabled = false;
					eraseCookie('login');
					eraseCookie('auth');
					
					Effect.toggle('welcome', 'blind', {queue: 'front', duration: '.25'}) ;
					Effect.toggle('loginForm', 'blind', {queue: 'end', duration: '.25'});
					scroll(0,0);      
					
					document.getElementById("LoginOut").innerHTML = " ";
					document.getElementById("btn_logout").style.cursor = "default";
					document.body.style.cursor = "default";
				}
				if (document.getElementById('CommentLoginForm') !== null) {
					document.body.style.cursor = "wait";
					
					document.getElementById("CommentLoginForm").disabled = false;
					eraseCookie('login');
					eraseCookie('auth');
					Effect.toggle('leaveComment', 'blind', {queue: 'front', duration: '.25'}) ;
					Effect.toggle('CommentLoginForm', 'blind', {queue: 'end', duration: '.25'});
					document.getElementById("LoginOut").innerHTML = " ";
					document.getElementById("CommentBtn_logout").style.cursor = "default";
					document.body.style.cursor = "default";
				}
			}
		
			
		function readCookie(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}
		
		function createCookie(name,value,days) {
			var expires;
			if (days) {
				var date = new Date();
				date.setTime(date.getTime()+(days*24*60*60*1000));
				expires = "; expires="+date.toGMTString();
			}
			else expires = "";
			document.cookie = name+"="+value+expires+"; path=/";
		}
		
		function eraseCookie(name) {
			createCookie(name,"",-1);
		}

		function ShowHide(element, control, param) {
			if (param === "") {
				param = "inline";
			}
			var srcElement = document.getElementById(element);
			var srcControl = control;
			if(srcElement !== null) {
				if(srcControl == "hide") {
					srcElement.style.display='none';
				}
				else {
					srcElement.style.display=param;
				}
				return false;
			}
		}  
		
	function flip(j) {
				Effect.toggle('Comment-'+j, 'blind', {queue: 'front', duration: '.25'});
				Effect.toggle('Comment-'+j+'-Full', 'blind', {queue: 'end', duration: '.25'});
		}

		function flop(j) {
				Effect.toggle('Comment-'+j+'-Full', 'blind', {queue: 'front', duration: '.25'});
				Effect.toggle('Comment-'+j, 'blind', {queue: 'end', duration: '.25'});
		}
