    commentLib = new GyrobaseAjaxLibrary("/ajax/commentMacros");

    function logInComment() {

      document.getElementById("CommentLoginForm").disabled = true;
      var userField = document.getElementById("loginUserName");
      var passField = document.getElementById("loginPassword");
      var user = userField.value;
      var pass = passField.value;
      document.getElementById("loginPassword").value = '';

	  
	        
	  var params = { "loginUserName": user, "loginPassword": pass, "PublicKey": "1579880404", "hash": "64ad62e56218d85ec1a73e11dc654f73" };

		passwordLib.runMacro("authenticate", params, 
			function (response) { 
				verifyLoginComment(response["status"],response["auth"],response["user"],response["Cookie"],response["Handle"],0);
			},
			function (error) {
				var ifrm = document.createElement("IFRAME");
				ifrm.setAttribute("src", "/sacramento/WhackBack?return=/sacramento/best-worst-of-wide/content%3Foid%3D29488708");
				ifrm.style.width = 0+"px";
				ifrm.style.height = 0+"px";
				document.body.appendChild(ifrm); 
				alert("This page generated an error.\n\nThe page is being rebuilt, and you will be redirected to the fresh page momentarily.\n\nPlease try again.");
				setTimeout('window.location.reload(true);',2000);
			}
		);
    }


	function postAnonymousComment(){

		document.getElementById("CommentLoginForm").disabled = true;
		var userField = document.getElementById("anonUserName");
		var user = userField.value;
		var siteID = document.getElementById("siteID").value;
		
		
		      
		var params = { "loginUserName": user, "Site": siteID, "PublicKey": "1579880404", "hash": "3b082120b719beda37df85aeabc0351d" };

		passwordLib.runMacro("authenticateAnon", params, 
			function (response) { 
				if (response["ErrorMsg"]){
					alert(response["ErrorMsg"]);
				} else {
				verifyLoginComment(response["status"],response["auth"],response["user"],response["Cookie"],response["Handle"],response["Anon"]);
			}},
			function (error) {
				var ifrm = document.createElement("IFRAME");
				ifrm.setAttribute("src", "/sacramento/WhackBack?return=/sacramento/best-worst-of-wide/content%3Foid%3D29488708");
				ifrm.style.width = 0+"px";
				ifrm.style.height = 0+"px";
				document.body.appendChild(ifrm); 
				alert("This page generated an error.\n\nThe page is being rebuilt, and you will be redirected to the fresh page momentarily.\n\nPlease try again.");
				setTimeout('window.location.reload(true);',2000);
			}
		);
	}


	function verifyLoginComment(status,auth,user,Cookie,Handle,Anon) {
		var f3 = document.getElementById("f3");
		if (auth != "") {
			if (status == 0) {
				alert("Your account has not been activated yet.\n\nPlease activate your account before proceeding.");
				
					var ifrm = document.createElement("IFRAME");
					//ifrm.setAttribute("src", "/sacramento/WhackBack?return=/sacramento/local/MyNR");
					ifrm.style.width = 0+"px";
					ifrm.style.height = 0+"px";
					document.body.appendChild(ifrm);
					ifrm.src = '/sacramento/WhackBack?return=/sacramento/local/MyNR';
					//alert("This page generated an error.\n\nThe page is being rebuilt, and you will be redirected to the fresh page momentarily.");
					//setTimeout('window.location.reload(true);',2000);			
					setTimeout(document.body.removeChild(ifrm),1000);
					document.getElementById('auth').value = auth;
					eraseCookie('login');
					createCookie('login',user + ':' + Cookie,1);
					createCookie('auth',auth,1);
					setTimeout(window.open('/sacramento/local/MyNR','ActivateWindow'),2000);
			} else {
				document.getElementById('auth').value = auth;
				document.getElementById('Anon').value = Anon;
				eraseCookie('login');
				createCookie('login',user + ':' + Cookie,1);
				createCookie('auth',auth,1);
				document.getElementById('CommentUser').innerHTML = ',&nbsp;' + Handle;
				Effect.toggle('CommentLoginForm', 'blind', {queue: 'front', duration: '.25'});
				Effect.toggle('leaveComment', 'blind', {queue: 'end', duration: '.25'});
				document.getElementById('LoginOut').innerHTML = '<a href="/sacramento/Login?logout=1&amp;pub=sacramento" class="footer">logout</a>&nbsp;|&nbsp;<a href="/sacramento/Login?changePass=1" class="footer">change password</a><br />';
				document.getElementById("CommentBtn_logout").style.cursor = "default";
				document.body.style.cursor = "default";
				setTimeout('window.scrollBy(0,300)',1000);
			}
		} else {
			if (Anon == 0) {
				alert("The password you entered doesn't match the password on file for the username you entered.  Please try again.");
			} else {
				alert("You cannot login anonymously with an email address that is already being used as a registered user on our system.");
			};
		}
		f3.login_btn.disabled = false;
	}
    
    function trim(stringToTrim) {
		return stringToTrim.replace(/^\s+|\s+$/g,"");
	}
    
	function cursor_wait() {
	  document.body.style.cursor = 'wait';
	}
	
	function cursor_clear() {
	  document.body.style.cursor = 'default';
	}
    
	function postComment() {
		cursor_wait();
		document.getElementById('submit_comment').disabled = true;
		var fullName = "";
		var phone = "";
		var city = "";
		var letter = 0;
		var error = "";
		var auth = document.getElementById('auth').value;
		var Anon = document.getElementById('Anon').value;
		var comment = document.getElementById('comment').value;
		if (document.getElementById('letterEdit') != null) {
			if (document.getElementById('letterEdit').checked == true) {
				letter = 1;
				fullName = document.getElementById('fullName').value;
				phone = document.getElementById('phone').value;
				city = document.getElementById('city').value;
				if(fullName == ''){
					error = "Please provide your full name.";
				}
				if(phone == ''){
					error = error + "\nPlease provide your phone number.";
				}
				if(city == ''){
					error = error + "\nPlease provide your city.";
				}
				if(error != '') {
					document.getElementById('submit_comment').disabled = false;
					cursor_clear();
					alert(error);
					return false;
				}
			}
		}
        if (trim(comment) != '') {
			
			
			var params = { 
				"ContentOid": '29488708', 
				"auth": auth, 
				"Anon": Anon,
				"comment": comment, 
				"letter": letter, 
				"fullName": fullName,
				"phone": phone,
				"city": city,
				"PublicKey": "1579880404", 
				"hash": "bd2323526c99cb4ed43a0ca1cc6396cd" 
			};
			commentLib.runMacro("postComment", params, function (response) { 
				if (response["status"] == 1) {
					document.getElementById('comment').value = "";
					if(letter == 1) {
						Effect.toggle('persInfo', 'blind', {queue: 'front', duration: '.25'});
						document.getElementById('fullName').value = '';
						document.getElementById('phone').value = '';
						document.getElementById('city').value = '';
						document.getElementById('letterEdit').checked = false;
					}
					document.getElementById('submit_comment').disabled = false;
					cursor_clear();
					if(response["moderate"] == 1) {
						if (response["Anonymous"]==1) {
							alert("Your comment has been submitted, and is awaiting moderation.\n\nPlease remember to check your email and follow the link provided to validate your email address.\n\nYour comment will not appear on the site until you follow the link provided.");
							logout();
						} else {
							alert("Your comment has been submitted, and is awating moderation");
						}
					} else {
						if (response["Anonymous"]==1) {
							alert("Thank you for your comment.\n\nPlease remember to check your email and follow the link provided to validate your email address.\n\nYour comment will not appear on the site until you follow the link provided.");
							document.getElementById('anonUserName').value = '';
							logout();
						} else {
							alert("Thank you for your comment.");
							setTimeout('window.location.reload(true);',1000);
						}
					}
					if (response["Anonymous"]==0) {
						getComments();
					}
				} else { 
					document.getElementById('submit_comment').disabled = false;
					cursor_clear();
					alert("Error: "+response["Error"]);
				}
        	})
        } else {
        	document.getElementById('submit_comment').disabled = false;
        	cursor_clear();
         	alert("Please enter a comment before submitting.");
	    }
	}
    
	function getComments() {
		var auth = document.getElementById('auth').value;
		
		
		var params = { "auth":auth, "ContentOid": '29488708', "ScriptAlias":'sacramento', "PublicKey": "1579880404", "hash": "c9a4867b1a479d3cf9735197bfff3983" };
		commentLib.runMacro("getComment", params, function (response) { updateComments(response["CommentResults"],response["error"],response["NumComments"]) });
	}
    
    function updateComments(Comments,error,NumComments){
    	if (!error) {
    		document.getElementById('CommentBody').innerHTML = Comments;
    		if (NumComments > 0) {
    			var tmpOut = '<br /><a href="#readComment">Read '+NumComments+' reader submitted comment';
    		    if (NumComments > 1) {
    		    	tmpOut = tmpOut + 's';
    		    }
    		    tmpOut = tmpOut + '</a><br /><br />';
    		    document.getElementById('NumComments').innerHTML = tmpOut;
			}
		}
    }
    
    function toggleInfo(){
    	Effect.toggle('persInfo', 'blind', {queue: 'front', duration: '.25'});
    }
</script>


