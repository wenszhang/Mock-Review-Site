<!-- Amazon A9 -->
<script type="text/javascript">
	function fetchHeaderBids() {

		var TIMEOUT = 2000;

		//declare bidders
		var bidders = ['a9', 'prebid'];

		// create a requestManager to keep track of bidder state to determine when to send ad server
		// request and what apstagSlots to request from the ad server
		var requestManager = {
			adserverRequestSent: false,
			apstagSlots: apstagSlots,
			adUnits: adUnits
		};

		//loop through bidder array and add the bidders to the request manager:
		bidders.forEach(function (bidder) {
			requestManager[bidder] = false;
		});

		// return true if all bidders have returned
		function allBiddersBack() {
			var allBiddersBack = bidders
			// get the booleans from the object
				.map(function (bidder) {
					return requestManager[bidder];
				})
				// get rid of false values - indicates that the bidder has responded
				.filter(Boolean)
				// if length is equal to bidders, all bidders are back
				.length === bidders.length;
			return allBiddersBack;
		}

		// handler for header bidder responses
		function headerBidderBack(bidder) {
			// return early if request to adserver is already sent
			if (requestManager.adserverRequestSent === true) {
				return;
			}
			// set the bidder targeting and flip bidder back flag
			if (bidder === 'a9') {
				requestManager.a9 = true;
			} else if (bidder === 'prebid') {
				requestManager.prebid = true;
			}

			// if all bidders are back, send the request to the ad server
			if (allBiddersBack()) {
				sendAdserverRequest();
			}
		}

		// actually get ads from DFP
		function sendAdserverRequest() {

			// return early if request already sent
			if (requestManager.adserverRequestSent === true) {
				return;
			}

			// flip the boolean that keeps track of whether the adserver request was sent
			requestManager.adserverRequestSent = true;

			// make ad request to DFP
			googletag.cmd.push(function () {
				apstag.setDisplayBids();
				pbjs.setTargetingForGPTAsync();
				googletag.pubads().refresh();
			});
		}

		function requestBids() {

			// request bids from prebid
			pbjs.que.push(function () {
				pbjs.addAdUnits(requestManager.adUnits);
				pbjs.setConfig({
					sizeConfig: [
												{
							'mediaQuery': '(min-width: 1622px)',
							'sizesSupported': [
								[970, 66],
								[970, 90],
								[970, 250],
								[970, 415],
								[728, 90],
								[300, 250],
								[300, 600],
								[1024, 90]
							],
							'labels': ['large_3']
						},
						{
							'mediaQuery': '(min-width: 992px) and (max-width: 1621px)',
							'sizesSupported': [
								[970, 66],
								[970, 90],
								[970, 250],
								[728, 90],
								[300, 250],
								[300, 600]
							],
							'labels': ['large_2']
						},
						{
							'mediaQuery': '(min-width: 750px) and (max-width: 991px)',
							'sizesSupported': [
								[300, 250],
								[300, 600],
								[768, 90]
							],
							'labels': ['large_1']
						},
						{
							'mediaQuery': '(min-width: 728px) and (max-width: 749px)',
							'sizesSupported': [
								[300, 250],
								[320, 50],
								[320, 100]
							],
							'labels': ['medium_2']
						},
						{
							'mediaQuery': '(min-width: 635px) and (max-width: 727px)',
							'sizesSupported': [
								[300, 250],
								[320, 50],
								[320, 100]
							],
							'labels': ['medium_1']
						},
						{
							'mediaQuery': '(min-width: 580px) and (max-width: 634px)',
							'sizesSupported': [
								[300, 250],
								[320, 50],
								[320, 100]
							],
							'labels': ['small_2']
						},
						{
							'mediaQuery': '(min-width: 0px) and (max-width: 579px)',
							'sizesSupported': [
								[300, 250],
								[320, 50],
								[320, 100]
							]
							,
							'labels': ['small_1']
						}
												]
				});
				pbjs.requestBids({
					bidsBackHandler: function (bidResponses) {
						headerBidderBack('prebid');
					}
				});
			});

			// fetch bids from APS
			apstag.fetchBids({
				slots: requestManager.apstagSlots
			}, function (bids) {
				headerBidderBack('a9');
			});
		}

		requestBids();
		// set timeout to send request to call sendAdserverRequest() after timeout
		// if all bidders haven't returned before then
		window.setTimeout(function () {
			sendAdserverRequest();
		}, TIMEOUT);
	};

	// Wait for: apstag
	deferUntil(function() {
		return typeof apstag != 'undefined';
	}, function() {
		//initialize the apstag.js library on the page to allow bidding
		apstag.init({
			pubID: '076b6f38-c5e1-40ef-8e28-908bb89612b2',
			adServer: 'googletag'
		});

		fetchHeaderBids(apstagSlots, adUnits);
	});
</script>
<script type='text/javascript'>
	var PREBID_TIMEOUT = 1200;
	nextSlotId = 9;

	/**
	 * Helper function for dynamic ads that generates slot names on the fly
	 * @returns {number}
	 */
	function generateNextSlotName() {
		var id = nextSlotId++;
		return id;
	}

	/**
	 *  Refresh an ad by its unit path
	 */
	function refreshAd( slotNumber, adUnitPath ) {
		// console.info('NYOVMG-ADS: Refreshing Ad', slotNumber, adUnitPath);
		if ( ! gptAdSlots[slotNumber] ) {
			// console.info('NYOVMG-ADS: Ad Slot Not Found', slotNumber, gptAdSlots);
			return;
		}

		// Refresh the bid for that ad slot
		var adSlot = gptAdSlots[slotNumber];
		refreshBid( adSlot.getSlotElementId(), adSlot );
	}

	/**
	 * Refresh Bids
	 * @param adSlot
	 * @param adUnit
	 */
	function refreshBid(adSlot, adUnitName) {
		// console.info('NYOVMG-ADS: Refreshing Bid', adSlot, adUnit);
		pbjs.que.push(function () {
			pbjs.requestBids({
				timeout: PREBID_TIMEOUT,
				adUnitCodes: [adSlot],
				bidsBackHandler: function () {
					// console.info('NYOVMG-ADS: bid back handler triggered');
					pbjs.setTargetingForGPTAsync([adSlot]);
					googletag.pubads().refresh([adUnitName], { changeCorrelator: false });
				}
			});
		});
	}

	/**
	 * Dynamically create an ad
	 *
	 * @param adUnitPath
	 * @param div
	 * @param size_mapping
	 * @param refresh
	 */
	function useReserveSlot( adUnitPath, div, size_mapping, refresh = true, adSlotNumber ) {
		var adSlot = generateNextSlotName(); // @NOTE - this is generated and then thrown away?
		adSlot = googletag.defineSlot(adUnitPath, [], div).defineSizeMapping(size_mapping).setCollapseEmptyDiv(true).addService(googletag.pubads());

		googletag.display(div);

		if ( refresh ) {
			refreshBid(adSlot.getSlotElementId(), adSlot);
		}

		// Store the ad slot in gptAdSlots if required
		if ( adSlotNumber && ( ! gptAdSlots[adSlotNumber]) ) {
			gptAdSlots[adSlotNumber] = adSlot;
		}
	};

	/**
	 * Dummy Timmer that refreshes two ads every 20seconds
	 */
	window.ad_refresh_timeout = 20000;
	// window.ad_refresh_timeout = 5000; // Debug testing

	function canRefreshFixedArticle() {
		// The page must be in focus & the user not "inactive"
		if ( ! window.page_is_focused || window.user_is_inactive ) {
			return false;
		}

		// The browser must be >= 770px wide
		var windowWidth = jQuery( window ).width();
		if ( windowWidth < 770 ) {
			return false;
		}

		// All good
		return true;
	}

	function refreshAds() {
		// Refresh the ads that need it
		// console.info('NYOVMG-ADS: Refreshing Ads');
		googletag.cmd.push(function() {
							// console.info('NYOVMG-ADS: pushing refresh for single article');
				if ( canRefreshFixedArticle() ) {
					// console.info('NYOVMG-ADS: pushing refresh for single fixed article');
					refreshAd( 3, '/1008049/observer_fixedarticle_1' );
				}
			
					});
	}

	// Wait for: jQuery
	deferUntil(function() {
		return typeof jQuery != 'undefined';
	}, function() {
		jQuery(document).ready(function() {
			// Setup the repeating refresh
			// console.info('NYOVMG-ADS: Setting Up Interval', ad_refresh_timeout);
			setInterval(refreshAds, ad_refresh_timeout);
		});
	});
</script>
